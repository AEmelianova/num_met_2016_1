    "def Adams(func1,func2,a,b,h0,ystart,y1start,eps):\n",
    "    #j - кол-во проходов цикла:\n",
    "    j = 0\n",
    "    \n",
    "    #Находим значение недостающих 2-х точек:    \n",
    "    RK = RKm5(f,f1,0,2,0.2,1,0,0.01)\n",
    "    ystart1 = RK[1][1]\n",
    "    y1start1 = RK[2][1]   \n",
    "    \n",
    "    while True: \n",
    "        h = h0/2**j\n",
    "        N = int(1+abs(b-a)/h)\n",
    "        w_N=np.linspace(a, b, N)\n",
    "        X = list(w_N)\n",
    "\n",
    "        if j==0:\n",
    "            hj0 = 2*h\n",
    "            Nj0 = int(1+abs(b-a)/hj0)                       \n",
    "            PrevY = [0 for i in range(0,Nj0)]\n",
    "            PrevY1 = [0 for i in range(0,Nj0)]\n",
    "        else:\n",
    "            PrevY = Y\n",
    "            PrevY1 = Y1\n",
    "\n",
    "        Y = [ystart, ystart1]\n",
    "        Y1 = [y1start,y1start1]\n",
    "\n",
    "        for i in range (2, N):\n",
    "            Y.append(Y[i-1] + h/2*(3*func1(X[i-1],Y[i-1],Y1[i-1])-func1(X[i-2],Y[i-2],Y1[i-2])))\n",
    "            Y1.append(Y1[i-1] + h/2*(3*func2(X[i-1],Y[i-1],Y1[i-1])-func2(X[i-2],Y[i-2],Y1[i-2])))\n",
    "        \n",
    "        for i in range(0,len(Y)):\n",
    "            if Y[i]==0:\n",
    "                Y[i]+=eps\n",
    "        for i in range(0,len(Y1)):\n",
    "            if Y1[i]==0:\n",
    "                Y1[i]+=eps\n",
    "                \n",
    "        Res = np.linalg.norm(abs(np.array(PrevY)-np.array(Y)[0::2])/np.array(Y)[0::2])\n",
    "        Res1 = np.linalg.norm(abs(np.array(PrevY1)-np.array(Y1)[0::2])/np.array(Y1)[0::2])\n",
    "        \n",
    "        j += 1\n",
    "        \n",
    "        if (Res < eps) and (Res1 < eps) and (j < 10) or (j > 10):\n",
    "            print('Шаг разбиения: ', h, 'Количество итераций (делений h0): ', j-1)\n",
    "            #print(Res, Res1)\n",
    "            break\n",
    "    return X, Y, Y1\n",
    "\n",
    "step = 0.0001\n",
    "Xgrid = np.arange(0, 2+step, step )\n",
    "\n",
    "A = Adams(f,f1,0,2,0.2,1,0,0.01)\n",
    "plt.plot(A[0], A[1], Xgrid, y(Xgrid))\n",
    "#plt.axis([0., 2, 0., 7])\n",
    "plt.legend(['$\\^y(x)$',\n",
    "            '$y(x)$'],    \n",
    "            loc=[1,0.5]) \n",
    "plt.xlabel('Axis $x$')\n",
    "plt.ylabel('Axis $y$')\n",
    "plt.title('Plot of the $y(x)$')\n",
    "plt.grid(True)"
   ]
